  <div class="search-bar">
    <%= form_with url: cards_search_results_path, method: "GET", local: true do %>
      <%= text_field_tag :search, params[:search], list: 'autocomplete', required: true, autocomplete: 'off', placeholder: 'Find a Card...'%>
      <datalist id="autocomplete"> </datalist>
    <% end %>
  </div>